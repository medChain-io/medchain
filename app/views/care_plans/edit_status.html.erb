<% @user = current_medwallet_account %>
<% @patient = Patient.find_by medwallet_id: @user.id  %>
<% @careplan = CarePlan.where patient_id: @patient.id %>
<div id="careplan_<%=@careplan.first.id%>_status" >
  <%= form_with(model: @careplan.first, local: false) do |form| %>
    <div class="field">
      <%= form.label :status, for: "#careplan_1539_status" %>
      <%= form.collection_select :status, Status.order(:id), :id, onchange: 'Rails.fire(this.form, "submit")', id: "#careplan_1539_status" %>
    </div>
  <% end %>
</div>